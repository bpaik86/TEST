/*-----------------------------------------------------------------*/
/* 1. Reset
/*-----------------------------------------------------------------*/

/* 웹폰트 적용 나눔고딕/나눔고딕볼드 */
@font-face{
	font-family: 'NanumGothicWeb';
	src: url('/font/NanumGothic.eot');
	src: local('?'), url('/font/NanumGothic.eot?#iefix') format('embedded-opentype'),
		url('/font/NanumGothic.woff') format('opentype'),
		url('/font/NanumGothic.ttf') format('truetype');
	}
@font-face{
	font-family: 'NanumGothicBoldWeb';
	src: url('/font/NanumGothicBold.eot');
	src: local('?'), url('/font/NanumGothicBold.eot?#iefix') format('embedded-opentype'),
		url('/font/NanumGothicBold.woff') format('opentype'),
		url('/font/NanumGothicBold.ttf') format('truetype');
}



/* HTML5 display-role reset for older browsers */
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section {display:block;}
body {line-height:normal;}
ol, ul, li {list-style:none;}
blockquote, q {quotes:none;}
blockquote:before, blockquote:after, q:before, q:after {content:''; content:none;}
table {border-collapse:collapse; border-spacing:0;}
li img {vertical-align:top;}
/*
input[type=radio],
input[type=checkbox] {position:relative; bottom:-2px;}
*/
input[type=text],
input[type=email],
input[type=password] {vertical-align:top;}
a {text-decoration:none;}
a:hover {color:#555; text-decoration:none;}
a:active, a:focus {text-decoration:none;}

body {
	line-height: 1;
	scrollbar-face-color: white;
	scrollbar-shadow-color: #c5c5c5;
	scrollbar-highlight-color: #c5c5c5;
	scrollbar-3dlight-color: white;
	scrollbar-darkshadow-color: white;
	scrollbar-track-color: #c5c5c5;
	scrollbar-arrow-color: #c5c5c5
}


